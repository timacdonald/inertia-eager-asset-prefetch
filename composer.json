{
    "name": "timacdonald/inertia-eager-asset-prefetch",
    "description": "Eager asset prefetching for Inertia, Laravel, and Vite. ",
    "keywords": [
        "inertia",
        "inertiajs",
        "laravel",
        "vite",
        "prefetch"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim MacDonald",
            "email": "hello@timacdonald.me",
            "homepage": "https://timacdonald.me"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "TiMacDonald\\InertiaEagerAssetPrefetch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TiMacDonald\\InertiaEagerAssetPrefetch\\ServiceProvider"
            ]
        }
    }
}
